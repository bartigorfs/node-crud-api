{
  "name": "node-crud-api",
  "author": "bartigorfs",
  "scripts": {
    "build": "webpack",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "start:prod": "cross-env NODE_ENV=production webpack && node dist/main.js",
    "start:multi": "cross-env BALANCER_NODES=4 webpack && node dist/main.js",
    "buildnrun": "webpack && node dist/main.js",
    "lint": "eslint --ext .ts .",
    "format": "prettier \"./**\" --write --ignore-unknown",
    "format:check": "prettier \"./**\" --ignore-unknown --check",
    "test": "jest --silent",
    "test:verbose": "jest"
  },
  "dependencies": {
    "tslib": "^2.8.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^28.0.0",
    "@jest/types": "^28.0.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^18.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "expect-type": "^1.1.0",
    "jest": "^27.5.1",
    "node-loader": "^2.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.1",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.5.1",
    "typescript": "^4.6.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
